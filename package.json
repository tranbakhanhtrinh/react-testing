{
  "name": "react-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage --watchAll --collectCoverageFrom='components/**/*.{ts,tsx}' --collectCoverageFrom='!components/**/*.{types,stories,constants,test,spec}.{ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.11",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "cross-fetch": "^4.0.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.2",
    "postcss": "latest",
    "prettier": "^3.0.3",
    "tailwindcss": "latest",
    "ts-jest": "^29.1.1",
    "typescript": "latest"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint"],
    "*.{ts,tsx,css,scss}": ["prettier --write"]
  }
}
